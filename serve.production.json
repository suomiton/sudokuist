{
	"public": "dist",
	"headers": [
		{
			"source": "**/*.@(js|css|html|wasm)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "**/*.@(js|css)",
			"headers": [
				{
					"key": "Content-Encoding",
					"value": "gzip"
				}
			]
		},
		{
			"source": "**/*.wasm",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/wasm"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/",
			"destination": "/index.html"
		}
	]
}
